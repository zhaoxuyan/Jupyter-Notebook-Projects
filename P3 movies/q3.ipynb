    "import pandas as pd\n",
    "movies = pd.read_csv('movies.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_novel(s):\n",
    "    if str(s).find('based on novel')!=-1:\n",
    "        return 'novel'\n",
    "    else:\n",
    "        return 'not novel'\n",
    "new = movies['keywords'].apply(find_novel)\n",
    "new.to_csv('is_novel.csv', index=False)"
   ]
